{
    "version": 2,
    "builds": [
      {
        "src": "your_project/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.x"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "your_project/wsgi.py"
      }
    ]
  }